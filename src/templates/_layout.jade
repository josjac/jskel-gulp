doctype

include _mixins

- var custom = {}

block variables

//if lt IE 7
  html#html.no-js.lt-ie9.lt-ie8.lt-ie7(lang='en')
//if IE 7
  html#html.no-js.lt-ie9.lt-ie8(lang='en')
//if IE 8
  html#html.no-js.lt-ie9(lang='en')
//if IE 9
  html#html.no-js.lt-ie10(lang='en')
//[if gt IE 9]><!
html#html.no-js(lang='en')
  //<![endif]

  head
    title 
    meta( charset='utf-8' )
    meta( http-equiv='X-UA-Compatible', content='IE=edge,chrome=1' )
    meta( name='description', content='' )

    meta( name='viewport',content='width=device-width,initial-scale=1.0, maximum-scale=1, user-scalable=no, minimal-ui')
    meta( name='format-detection', content='telephone=no')
    meta( name='apple-mobile-web-app-title', content='internet' )

    block css
      link( href=handler.static_url('styles/main.css'), rel='stylesheet', type='text/css', media='all' )

    block metas
      meta( property='og:title', content!='' )
      meta( property='og:url', content!='' )
      meta( property='og:image', content!=handler.static_url(''))
      meta( property='og:description', content!='' )

  body#body
    block body


    block javascript
